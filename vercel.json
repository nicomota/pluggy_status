{
  "version": 2,
  "builds": [
    {
      "src": "backend.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["conectorsMister.js"]
      }
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/backend.js"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}